{
  "manifest_version": 3,
  "name": "YouTube Watch Together",
  "version": "0.1",
  "description": "Toby's pathetic attempt at a YouTube Watch Together extension",
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "host_permissions": [
    "*://youtube.com/*"
  ]
}